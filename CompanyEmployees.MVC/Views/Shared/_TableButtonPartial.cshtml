@model Guid

<td >
    <div class="btn-group" role="group">
        <a class="btn btn-primary text-white" asp-action="Update" asp-route-Id="@Model">
            <i class="fas fa-edit"></i>
        </a>

        <a class="btn btn-success text-white" asp-action="Delete" asp-route-Id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>